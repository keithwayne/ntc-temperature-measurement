-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
SECONDARY_FLASH += \
NTC.hex \

SECONDARY_LIST += \
NTC.lst \

SECONDARY_SIZE += \
NTC.siz \


# All Target
all: 
	$(MAKE) --no-print-directory main-build 

main-build: NTC.elf secondary-outputs

# Tool invocations
NTC.elf: $(OBJS) $(USER_OBJS_ESCAPE)
	@	riscv-wch-elf-gcc -march=rv32ec_zmmul_xw -mabi=ilp32e -msmall-data-limit=0 -msave-restore -fmax-errors=20 -Os -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -fno-common -Wunused -Wuninitialized -g -T "d:/PROJECT/mounriver-studio-projects/ntc-temperature-measurement/NTC/Ld/Link.ld" -nostartfiles -Xlinker --gc-sections -Wl,-Map,"NTC.map" --specs=nano.specs --specs=nosys.specs -o "NTC.elf" $(OBJS) $(LIBS) -lprintf
NTC.hex: NTC.elf
	@	riscv-wch-elf-objcopy -O ihex "NTC.elf" "NTC.hex"
NTC.lst: NTC.elf
	@	riscv-wch-elf-objdump --all-headers --demangle --disassemble -M xw "NTC.elf" > "NTC.lst"
NTC.siz: NTC.elf
	riscv-wch-elf-size --format=berkeley "NTC.elf"

# Other Targets
clean:
	-$(RM) $(DIR_OBJS) $(SECONDARY_FLASH)$(SECONDARY_LIST)$(SECONDARY_SIZE) NTC.elf
	-$(RM) $(DIR_EXPANDS) $(CALLGRAPH_DOT)
	-$(RM) $(DIR_DEPS)
secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_LIST) $(SECONDARY_SIZE)

.PHONY: all clean dependents

